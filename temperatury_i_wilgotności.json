    {
        "id": "617705ed2c4a3a5b",
        "type": "function",
        "z": "3e6505394cf48b86",
        "name": "RHT temperatura",
        "func": "if (msg.payload.action === \"data.scan\"){\n    if (msg.payload.data[0].model === \"ELA_RHT\"){\n        msg.payload = msg.payload.data[0].charValues.temperature;\n    } else {\n        return;\n    }\n} else {\n    return;\n}\nmsg.topic = \"temperatura\";\nreturn msg;",
