    {
        "id": "f4dfd455a8778a43",
        "type": "function",
        "z": "3e6505394cf48b86",
        "name": "MAG state",
        "func": "if (msg.payload.action === \"data.scan\"){\n    if (msg.payload.data[0].model === \"ELA_MAG\"){\n        if (Object.keys(msg.payload.data[0].charValues).length > 0){\n            msg.payload = msg.payload.data[0].charValues.alertLevel.state;\n        } else {\n            return;\n        }      \n    } else {\n        return;\n    }\n} else {\n    return;\n}\nmsg.topic = \"stan\";\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
